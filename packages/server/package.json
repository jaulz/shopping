{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "start": "npm run compile && node ./dist/index.js",
    "dev": "concurrently -k -n \"mongo,apollo\" -c \"blue,magenta\" \"yarn database\" \"wait-on tcp:27017 && yarn apollo\"",
    "apollo": "tsx watch src/index.ts",
    "database": "docker run -i -p 27017:27017 mongo"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "apollo-datasource-mongodb": "^0.5.4",
    "graphql": "^16.6.0",
    "mongodb": "^4.13.0",
    "mongoose": "^6.9.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  }
}
